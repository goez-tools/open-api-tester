{
  "name": "goez/open-api-tester",
  "type": "library",
  "authors": [
    {
      "name": "John Huang",
      "email": "johnhuang@kkbox.com"
    },
    {
      "name": "Jace Ju",
      "email": "jaceju@gmail.com"
    }
  ],
  "license": "proprietary",
  "homepage": "https://web.kkbox.io/open-api-tester",
  "require": {
    "php": "7.4|^8.1",
    "cebe/php-openapi": "^1.7",
    "justinrainbow/json-schema": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Goez\\OpenAPI\\Tester\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laravel/pint": "^1.13",
    "orchestra/testbench": "^8.15"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Goez\\OpenAPI\\Tester\\OpenApiTesterServiceProvider"
      ]
    }
  },
  "suggest": {
    "sempro/phpunit-pretty-print": "To print the validation results of each endpoint"
  }
}
